programa
{
	
	funcao inicio()
	{
		escreva("Velocidade do carro = ")
		real vel
		leia(vel)

		se (vel > 80){
			escreva("Você vai ser ..: MULTADO!\n")
		}
		
		escreva("\nDirija com cuidado!\n")		
	}
}
